{
  "name": "kabira-mobility-platform",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for Kabira Mobility Booking Platform",
  "scripts": {
    "dev:web": "pnpm --filter @kabira/web dev",
    "dev:cms": "pnpm --filter @kabira/cms dev",
    "dev": "pnpm run \"/^dev:.*/\"",
    "build:web": "pnpm --filter @kabira/web build",
    "build:cms": "pnpm --filter @kabira/cms build",
    "build": "pnpm run build:cms && pnpm run build:web",
    "start:web": "pnpm --filter @kabira/web start",
    "start:cms": "pnpm --filter @kabira/cms start",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:web": "pnpm --filter @kabira/web lint",
    "lint:cms": "pnpm --filter @kabira/cms lint",
    "payload": "pnpm --filter @kabira/cms payload",
    "generate:types": "pnpm --filter @kabira/cms generate:types",
    "format": "prettier --write \"apps/**/*.{js,jsx,ts,tsx,json,css,md}\" \"packages/**/*.{js,jsx,ts,tsx,json,css,md}\" \"*.{js,json,md}\" --ignore-path .gitignore"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}